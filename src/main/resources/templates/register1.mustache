<script type="text/javascript">
$(document).ready(function() {
	//체크박스 포커싱
	$('.next-focus').click(function(){
		if ( $(this).attr('id') == 'agree1' ) {
			$('#agree2').siblings('label').focus();
		} else if ( $(this).attr('id') == 'agree2' ) {
			$('#confirmButton').focus();
		}
	});
	
	//submit 필수 약관 동의 확인
	$('#joinForm').submit(function(event){
		if (!$('#agree2').is(':checked')) {
			alert('이용약관과 개인정보 수집 및 이용에 대한 안내(필수)에 대해서\n동의해 주세요.');
			return false;
		}
	});

	//모바일일때 언어선택이 드롭다운리스트로 변환
	var mdlang = $('.nav_lang li.on').text();
	var mdlst = $('.selct');
	$(mdlst).children('a').text(mdlang);
	$(mdlst).click(function(){
		if ($(this).hasClass('on')) {
			$(this).removeClass('on');
		}else{
			$(this).addClass('on');
		}
	});
});
</script>
